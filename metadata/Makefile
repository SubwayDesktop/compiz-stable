all-am: Makefile $(DATA)
installdirs:
	for dir in "$(DESTDIR)$(schemadir)" "$(DESTDIR)$(xmldir)" "$(DESTDIR)$(xsltdir)"; do \
	  test -z "$$dir" || $(MKDIR_P) "$$dir"; \
	done
